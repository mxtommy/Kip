    "@types/node": "^24.1.0",
    "@types/d3": "^7.4.3",
    "angular-eslint": "21.2.0",
    "codelyzer": "^6.0.0",
    "eslint": "^9.29.0",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-spec-reporter": "^0.0.36",
    "ng-packagr": "^21.0.1",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^8.1.1",
    "sass": "^1.49.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
